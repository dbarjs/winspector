    "@types/ws": "^8.5.5",
    "c12": "^1.4.2",
    "changelogen": "^0.5.5",
    "chokidar": "^3.5.3",
    "citty": "^0.1.4",
    "clipboardy": "^3.0.0",
    "colorette": "^2.0.20",
    "consola": "^3.2.3",
    "destr": "^2.0.1",
    "eslint": "^8.49.0",
    "execa": "^8.0.1",
    "fuse.js": "^6.6.2",
    "giget": "^1.1.2",
    "h3": "^1.8.1",
    "httpxy": "^0.1.5",
