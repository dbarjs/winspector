    "listhen": "^1.5.5",
    "magicast": "^0.3.0",
    "mlly": "^1.4.2",
    "nuxt": "^3.7.4",
    "nypm": "^0.3.3",
    "ofetch": "^1.3.3",
    "pathe": "^1.1.1",
    "perfect-debounce": "^1.0.0",
    "pkg-types": "^1.0.3",
    "prettier": "^3.0.3",
    "scule": "^1.0.0",
    "semver": "^7.5.4",
    "unbuild": "^2.0.0",
    "unws": "^0.2.4",
    "ws": "^8.14.2",
    "nodemon": "3.0.1"
